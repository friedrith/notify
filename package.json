{
  "name": "notify",
  "version": "0.1.0",
  "description": "A easy-to-use command line interface to notify you when a very long terminal process is finished",
  "main": "index.js",
  "repository": "git@github.com:friedrith/notify.git",
  "author": "Thibault Friedrich <thibault.friedrich@gmail.com>",
  "license": "MIT",
  "private": false,
  "bin": {
    "notify": "./dist/bin.js"
  },
  "scripts": {
    "build": "webpack",
    "dev": "webpack --mode development --watch",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "prettier:check": "prettier --check 'src/**/*.js'",
    "prettier:fix": "prettier --write 'src/**/*.js'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/travis-cli": "^8.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.2.0",
    "husky": "^3.0.5",
    "prettier": "^1.18.2",
    "shebang-loader": "^0.0.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "commander": "^3.0.1",
    "node-notifier": "^5.4.3"
